package yarago;

public class Calsi {
 public void add(double a,double b) {
	 System.out.println("Sum of a and b = "+"\n"+(a+b));
 }
 public void sub(double a,double b) {
	 System.out.println("Sum of a and b = "+"\n"+(a+b));
 }
 public void multi(double a,double b) {
	 System.out.println("Sum of a and b = "+"\n"+(a+b));
 }
 public void div(double a,double b) {
	 System.out.println("Sum of a and b = "+"\n"+(a+b));
 }
}
